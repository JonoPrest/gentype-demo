{
  "name": "gentype",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "gentypeconfig": {
    "module": "esmodule",
    "generatedFileExtension": ".gen.ts"
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".js",
  "bs-dependencies": [
    "@rescript/core"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ]
}
